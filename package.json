    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "msw": "^2.12.4",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
